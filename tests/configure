#!/bin/sh
#
#
UNAME=uname
case `${UNAME} -a` in
  *MINGW* | *CYGWIN* )
  	  echo "Windows platform... using .buildinfo.win32"
  	  cp sqlite-tests.buildinfo.win32 sqlite-tests.buildinfo
	  ;;
  *)
  	  echo "UNIXy platform... using .buildinfo.unix"
  	  cp sqlite-tests.buildinfo.unix sqlite-tests.buildinfo
	  ;;
esac

